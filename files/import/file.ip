    <div class="modalPanel" id="modalPanelId_0" style="display:none;">&nbsp;</div><div class="modalPanel" id="modalPanelId_1" style="display:none;">&nbsp;</div><div class="modalPanel" id="modalPanelId_2" style="display:none;">&nbsp;</div><script type="text/javascript">

        function showModalPanel(panelId) {
            jQuery.noConflict()(
                               function($) {
                                   $('#' + panelId).jqmShow();
                                   $('#' + panelId).css({top:'50%',left:'50%',margin:'-' + ($('#' + panelId).height() / 2) + 'px 0 0 -' + ($('#' + panelId).width() / 2) + 'px'});
                               });
        }

        function hideModalPanel(panelId) {
            jQuery.noConflict()(
                               function($) {
                                   $('#' + panelId).jqmHide();
                               });
        }

        var currentModalNumber = -1;

        function showModalPanelWithRequest(ajaxRequest) {
            jQuery.noConflict()(
                               function($) {
                                   if ($.ajax) {
                                       $.ajax({
                                           url: ajaxRequest,
                                           dataType: 'html',
                                           beforeSend: function() {
                                               showContentInModalPanel("ЗАГРУЗКА....");
                                           },
                                           success: function(value) {
                                               hideCurrentModalPanel();
                                               showContentInModalPanel(value);
                                           },
                                           error: function() {
                                               hideCurrentModalPanel();
                                               alert('Сервер не отвечает, попробуйте еще раз');
                                           }
                                       })
                                   }
                               });
        }

        function showContentInModalPanel(content) {
            if (currentModalNumber + 1 != '3') {
                currentModalNumber = currentModalNumber + 1;

                jQuery.noConflict()(
                                   function($) {
                                       var selector = '#' + getModalId(currentModalNumber);
                                       $(selector).jqmShow();
                                       $(selector).html(content);
                                       $(selector).css({top:'50%',left:'50%',margin:'-' + ($(selector).height() / 2) + 'px 0 0 -' + ($(selector).width() / 2) + 'px'});
                                   });
            }
        }

        function hideCurrentModalPanel() {
            if (currentModalNumber != -1) {
                jQuery.noConflict()(
                                   function($) {
                                       $('#' + getModalId(currentModalNumber)).jqmHide();
                                       $('#' + getModalId(currentModalNumber)).html('&nbsp;');
                                   });
                currentModalNumber = currentModalNumber - 1;
            }
        }

        function getModalId(number) {
            return 'modalPanelId_' + number;
        }

        jQuery.noConflict()(
                           function($) {
                               $.jqm.params.modal = true;

                               $().ready(
                                        function() {
                                            for (var i = 0; i != '3'; i++) {
                                                $('#' + getModalId(i)).jqm({trigger:false});
                                            }
                                        });
                           });
    </script>
