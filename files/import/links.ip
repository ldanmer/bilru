    function placeIt(id)
    {
        //alert(id);
        theLeft = get_ww()/2 - 250;
        theTop = get_wh()/2 - 100;
        obj = document.getElementById(id);
        obj.style.left = theLeft + 'px' ;
        obj.style.top = theTop + 'px' ;
        window.setTimeout('placeIt("' + id + '");', 1000);
    }



